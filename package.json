{
  "name": "avweather-cli",
  "description": "Terminal application for getting aviation/pilot weather",
  "version": "0.4.3",
  "author": "David Fekke @davidfekke",
  "bin": {
    "avwx": "./bin/run"
  },
  "bugs": "https://github.com/davidfekke/avweather-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.1",
    "@oclif/config": "^1.7.6",
    "@oclif/plugin-help": "^2.1.2",
    "axios": "^0.18.1",
    "fast-xml-parser": "^3.12.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.18.1",
    "eslint": "^5.5.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/davidfekke/avweather-cli",
  "keywords": [
    "oclif",
    "metar",
    "taf",
    "aviation",
    "weather"
  ],
  "license": "Apache-2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "avwx",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "davidfekke/avweather-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
